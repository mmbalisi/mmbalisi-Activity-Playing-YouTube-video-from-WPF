<Window x:Class="MarkBalisi.PlayingYouTubeVideoFromWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarkBalisi.PlayingYouTubeVideoFromWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <WebBrowser x:Name="Browser" HorizontalAlignment="Left" Height="319" Margin="10,54,0,0" VerticalAlignment="Top" Width="772"/>
        <TextBox x:Name="txtVideoUrl" HorizontalAlignment="Left" Height="39" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="772"/>
        <Button x:Name="btnGo" Content="Go" HorizontalAlignment="Left" Height="31" Margin="624,378,0,0" VerticalAlignment="Top" Width="158" Click="btnGo_Click"/>

    </Grid>
</Window>
